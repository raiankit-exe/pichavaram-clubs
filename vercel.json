{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/", "dest": "server.js" },
    { "src": "/auth/google", "dest": "server.js" },
    { "src": "/auth/google/callback", "dest": "server.js" },
    { "src": "/logout", "dest": "server.js" }
  ]
} 
